import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner sc = new Scanner(System.in);
		System.out.println("Enter the two numbers:");
		int num1=sc.nextInt();
		int num2=sc.nextInt();
		
		System.out.println("NUMBER:" +(num1+num2));
		System.out.println("NUMBER:" +(num1-num2));
		System.out.println("NUMBER:" +(num1*num2));
		System.out.println("NUMBER:" +(num1/`num2));
		System.out.println("NUMBER:" +(num1%num2));
	}
}
